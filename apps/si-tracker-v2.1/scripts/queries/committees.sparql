# SLSC flags
PREFIX :   <https://id.parliament.uk/schema/>
SELECT DISTINCT ?workPackage (true AS ?slscFlag) ?slscReportURI
WHERE {
  ?workPackage :workPackagedThing ?thing . ?thing a :StatutoryInstrument .
  ?bi a :BusinessItem ; :workPackagedThing ?thing ; :businessItemHasProcedureStep ?step .
  ?step :preferredLabel ?stepLabel .
  FILTER ( CONTAINS(LCASE(?stepLabel), "secondary legislation scrutiny committee draws special attention") )
  OPTIONAL { ?bi :workPackagedThingPaperURI ?slscReportURI . }
}